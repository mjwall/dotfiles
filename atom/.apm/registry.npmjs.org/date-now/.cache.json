[2]: http://travis-ci.org/Raynos/date-now\n  [3]: http://ci.testling.com/Raynos/date-now.png\n  [4]: http://ci.testling.com/Raynos/date-now\n","maintainers":[{"name":"raynos","email":"raynos2@gmail.com"}],"time":{"modified":"2014-06-11T23:21:08.828Z","created":"2013-01-23T01:05:20.672Z","0.1.0":"2013-01-23T01:05:21.504Z","0.1.1":"2013-01-27T05:41:43.303Z","0.1.3":"2013-01-29T17:13:27.623Z","0.1.2":"2013-02-09T03:00:29.539Z","0.1.4":"2013-02-09T03:01:53.400Z","1.0.0":"2014-04-09T07:05:44.849Z","1.0.1":"2014-06-11T23:21:08.828Z"},"author":{"name":"Raynos","email":"raynos2@gmail.com"},"repository":{"type":"git","url":"git://github.com/Raynos/date-now.git"},"homepage":"https://github.com/Raynos/date-now","keywords":[],"contributors":[{"name":"Artem Shoobovych"}],"bugs":{"url":"https://github.com/Raynos/date-now/issues","email":"raynos2@gmail.com"},"readmeFilename":"README.md","users":{"rouabhi":true},"_attachments":{},"_etag":"\"4IVDPT1USQTTDVL345B8VOWJW\""}