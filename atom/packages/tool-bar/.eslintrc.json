{
  "parser": "babel-eslint",
  "extends": "standard",
  "globals": {
    "atom": false,
    "afterEach": false,
    "beforeEach": false,
    "describe": false,
    "expect": false,
    "it": false,
    "jasmine": false,
    "runs": false,
    "spyOn": false,
    "waitsFor": false,
    "waitsForPromise": false,
    "indexedDB": false
  },
  "rules": {
    "semi": [2, "always"]
  }
}
